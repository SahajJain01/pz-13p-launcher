{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "PZ 13th Pandemic",
  "version": "0.1.0",
  "identifier": "com.administrator.tauri-app",
  "build": {
    "beforeDevCommand": "bun run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "bun run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
  "title": "PZ 13th Pandemic",
        "width": 1021,
        "height": 574,
  "decorations": true,
  "resizable": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
